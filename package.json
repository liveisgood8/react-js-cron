{
  "name": "react-js-cron",
  "version": "1.1.0",
  "description": "A React cron editor with antd inspired by jqCron",
  "author": "Xavier Rutayisire <xavier.rutayisire@gmail.com> (https://github.com/xrutayisire/)",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "js",
    "cron",
    "crontab",
    "editor",
    "widget",
    "generator",
    "antd",
    "ant-design"
  ],
  "homepage": "https://github.com/xrutayisire/react-js-cron#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xrutayisire/react-js-cron.git"
  },
  "bugs": {
    "url": "https://github.com/xrutayisire/react-js-cron/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "del dist && npm run build:babel && npm run build:types",
    "build:babel": "npm run build:babel:esm && npm run build:babel:cjs",
    "build:babel:cjs": "set BABEL_ENV=cjs&&babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/cjs --presets=@babel/env",
    "build:babel:esm": "set BABEL_ENV=esm&&babel --extensions \".ts,.tsx\" --copy-files src --out-dir dist/esm",
    "build:types": "tsc",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prettier": "prettier --write 'src/**/*.{ts?(x),css}'",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "antd": ">=4.6.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.1.0",
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.9.6",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "antd": "^4.6.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^1.3.2",
    "concurrently": "^5.2.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.9.3"
  }
}
